{
  "name": "cps-resume",
  "version": "1.0.0",
  "description": "A resume",
  "main": "index.js",
  "repository": {
    "type": "git"
  },
  "keywords": [
    "resume"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "lint": "eslint src/js/**.*",
    "watch-js": "watchify ./src/js/app.jsx -v -d -o ./dist/bundle.js",
    "watch-css": "node-sass --watch ./src/scss/app.scss -o ./dist/css",
    "watch": "concurrent --kill-others --prefix command --prefix-length 50 \"npm run watch-css\" \"npm run watch-js\"",
    "prestart": "rm -rf dist && mkdir -p dist",
    "beefy": "beefy ./src/js/app.jsx:bundle.js -t --open --cw .",
    "server": "npm run lint && concurrent --kill-others --prefix command --prefix-length 50 \"npm run watch-css\" \"npm run beefy\"",
    "build-js": "browserify -t ./src/js/app.jsx | uglifyjs -cm > ./dist/bundle.min.js",
    "build-css": "node-sass ./src/scss/app.scss -o ./dist/css",
    "build": "npm run lint && npm run build-js && npm run build-css",
    "clean": "rm -rf ./node_modules && npm install",
    "start": "npm install && npm run build-css && npm run server"
  },
  "dependencies": {
    "babel-eslint": "^4.1.3",
    "babelify": "^6.1.3",
    "beefy": "^2.1.5",
    "bootstrap-sass": "^3.3.5",
    "browserify": "^12.0.1",
    "concurrently": "^0.1.1",
    "eslint": "^1.8.0",
    "eslint-plugin-react": "^3.6.3",
    "lodash": "^3.10.1",
    "node-sass": "^3.4.1",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "uglify-js": "^2.5.0",
    "watchify": "^3.6.0"
  },
  "author": {
    "name": "Casey Sullivan",
    "email": "casey.sullivan@gmail.com",
    "url": "http://caseysullivan.net"
  }
}
